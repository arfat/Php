<?xml version="1.0" encoding="UTF-8"?><Module>	<data>		<!-- All the global paths defined for the product goes in the global nodes -->		<global>						<!-- imagePath node contains the path for the images folder 					http://192.168.20.8:8080/radi_php/slideshow/					file:///Volumes/GlamData/GLAM/Glam%20Projects/AdProdcutDev/FBProjects/SplashClassic300x250			-->									<imagePath><![CDATA[]]>../assets/</imagePath>			<!-- videoPath node contains the path for the video folder -->			<videoPath><![CDATA[]]>			</videoPath>			<!-- templatesPath node contains the path for the templates folder -->			<templatesPath><![CDATA[]]>			</templatesPath>			<!-- xmlPath node contains the path for the xml folder -->			<xmlPath><![CDATA[]]>			</xmlPath>			<!-- transitionPath node contains the path for the transition folder -->			<transitionPath><![CDATA[]]>			</transitionPath>		</global>		<interfaceSettings>			<global fullscreen="on" autoplay="off" autoplaytimer="5" />			<logo vis="on" src="400000014-logo.jpg" file="images.jpg"				u="http://www.glam.com" />			<!-- theme is contains theme parameters				type should be [swf].[image],[custom]				> if theme type=swf/image then it takes theme from name and add _cl, _fs,_lta,					_rta,_bg from banner itself with the extention swf/jpg.				> if theme type=custom then it takes theme from inner node with file name.			-->			<theme name="def" type="swf">				<content name="bg" type="swf" src="bg.swf" />				<content name="close" type="swf" src="cl.swf" />				<content name="fullscreen" type="swf" src="fs.swf" />				<content name="leftarrow" type="swf" src="lta.swf" />				<content name="rightarrow" type="swf" src="rta.swf" />			</theme>			<transitions type="random">				<transition id="type01" path="trans01.swf" />				<transition id="type02" path="trans02.swf" />				<transition id="type03" path="trans03.swf" />				<transition id="type04" path="trans04.swf" />				<transition id="type04" path="trans05.swf" />			</transitions>			<templates>				<template id="image" path="imageLoader.swf" />				<template id="video" path="videoPlayer.swf" />				<template id="swf" path="swfLoader.swf" />				<template id="dummy01" path="dummy01.swf" />				<template id="dummy02" path="dummy02.swf" />			</templates>			<!-- transition node contains the information for the slide transitions 				the entries could be [random],[fadeInOut],[slidein],[slideout] -->			<transition><![CDATA[random]]></transition>			<!-- minimized view contains the 300x250 view information and placement 				of the elements in the product -->			<defaultView>				<!-- dockPosition node contains the position of the dock. we can place 					it [bottom],[top],[right],[left] -->				<dock location="left" size="155,25" type="horizontal" floating="true"					rows="1" />				<!-- containerPosition node contains the alignment of the main display 					object container. [center],[topLeft],[topRight],[bottomLeft],[bottomRight] -->				<containerPosition align="center" />			</defaultView>			<!-- maximized view contains the fullscreen view information and placement 				of the elements in the product -->			<fullscreenView>				<!-- dockPosition node contains the position of the dock. we can place 					it [bottom],[top],[right],[left] -->				<dock location="right" size="110,200" type="vertical" floating="false"					columns="2" />				<!-- containerPosition node contains the alignment of the main display 					object container. [center],[topLeft],[topRight],[bottomLeft],[bottomRight] -->				<containerPosition align="center" />			</fullscreenView>			<!-- sharing node contains the icon link to share the unit infomation 				on social networks -->			<sharing>				<share name="facebookShare"					url="http://share.glam.com/share.php?cid=400000014&amp;t=Splash HD for Demo&amp;sz=300x250"					xPos="240" yPos="260" vis="on" />				<share name="facebookLike" url="http://www.facebook.com/glamcom"					xPos="200" yPos="260" vis="off" />				<share name="tweet"					url="http://share.glam.com/share.php?cid=400000014&amp;t=Splash HD for Demo&amp;sz=300x250"					xPos="220" yPos="260" vis="on" />				<share name="tweetFollow" url="http://twitter.com/onglamfashion"					xPos="260" yPos="260" vis="off" />			</sharing>		</interfaceSettings>		<slides>			<slide type="video" src="img6.flv" u="http://www.glam.com"				name="Slide 1" file="img06.flv"><![CDATA[Closed Caption 1]]></slide>			<slide type="image" src="img2.jpg" u="http://www.glam.com"				name="Slide 2" file="img02.jpg"><![CDATA[Closed Caption 2]]></slide>			<slide type="swf" src="swf3.swf" u="http://www.glammedia.com"				name="Slide 3" file="swf03.swf"><![CDATA[Closed Caption 3]]></slide>			<slide type="video" src="video.flv" u="http://www.glam.com"				name="Slide 4" file="video.flv"><![CDATA[Closed Caption 4]]></slide>			<slide type="image" src="img5.jpg" u="http://www.glam.com"				name="Slide 5" file="img05.jpg"><![CDATA[Closed Caption 5]]></slide>			<slide type="image" src="img1.jpg" u="http://www.glam.com"				name="Slide 6" file="img01.jpg"><![CDATA[Closed Caption 6]]></slide>			<slide type="video" src="img7.flv" u="http://www.glammedia.com"				name="Slide 7" file="img07.flv"><![CDATA[Closed Caption 7]]></slide>			<slide type="image" src="img8.jpg" u="http://www.glam.com"				name="Slide 8" file="img08.jpg"><![CDATA[Closed Caption 8]]></slide>		</slides>		<tot n="8" />		<!-- unexpanded Assets	-->		<assets>			<asset type="swf" src="unexpanded.swf" u="http://www.glam.com"				name="start_ani" file="unexpanded.swf"></asset>			<asset type="swf" src="timer.swf" u="http://www.glam.com"				name="timer" file="timer.swf"></asset>			<asset type="image" src="header.jpg" u="http://www.glam.com"				name="header" file="header.jpg"></asset>						</assets>	</data></Module>